<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" xml:lang="pt-BR" class="note" demote-non-dropping-particle="never">
  <info>
    <title>PIBIC_UNIOESTE (Estilo Personalizado PRPPG)</title>
    <id>http://www.zotero.org/styles/pibic_unioeste</id>
    <link href="http://www.zotero.org/styles/pibic_unioeste" rel="self"/>
    <link href="http://www.zotero.org/styles/apa" rel="template"/>
    <author>
      <name>Adaptado por ChatGPT para PRPPG - Unioeste</name>
    </author>
    <updated>2025-05-11T00:00:00+00:00</updated>
    <category citation-format="author-date"/>
    <category field="generic-base"/>
    <summary>Estilo CSL adaptado às normas da PRPPG (PIBIC/UNIOESTE), baseado no APA 6ª edição.</summary>
    <language>pt-BR</language>
  </info>
  <!-- O conteúdo completo do estilo ainda deve ser personalizado no editor visual CSL -->

<!-- Tese ou Dissertação -->
<macro name="thesis">
  <text variable="title" font-style="italic"/>
  <text value=". "/>
  <group delimiter=", ">
    <text variable="genre"/>
    <text variable="publisher"/>
  </group>
</macro>

<!-- Capítulo de Livro -->
<macro name="chapter">
  <text variable="title"/>
  <text value=". In "/>
  <names variable="editor">
    <name and="symbol" initialize-with="." delimiter=", "/>
    <label form="short" prefix=" (" suffix=")"/>
  </names>
  <text variable="container-title" font-style="italic" prefix=", "/>
  <group prefix=" (" suffix=")" delimiter=" ">
    <label variable="page" form="short"/>
    <text variable="page"/>
  </group>
  <text variable="publisher" prefix=". " />
</macro>

<!-- Portaria -->
<macro name="portaria">
  <text variable="title"/>
  <text variable="number" prefix=". Portaria " suffix="."/>
  <date variable="issued" prefix=" " suffix=".">
    <date-part name="day" suffix=" de "/>
    <date-part name="month" suffix=" de "/>
    <date-part name="year"/>
  </date>
  <text variable="container-title" prefix=" " suffix="."/>
  <text variable="page" prefix=" " suffix="."/>
</macro>

<!-- Resolução -->
<macro name="resolucao">
  <text variable="title"/>
  <text variable="number" prefix=". Resolução " suffix="."/>
  <date variable="issued" prefix=" " suffix=".">
    <date-part name="day" suffix=" de "/>
    <date-part name="month" suffix=" de "/>
    <date-part name="year"/>
  </date>
  <text variable="container-title" prefix=" " suffix="."/>
  <text variable="page" prefix=" " suffix="."/>
</macro>

</style>
